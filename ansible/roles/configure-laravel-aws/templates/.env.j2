APP_NAME=Laravel
APP_ENV=production
APP_KEY=base64:{{ lookup('password', '/dev/null length=32 chars=ascii_letters,digits') }}
APP_DEBUG=false
APP_URL=http://{{ server_name }}

DB_CONNECTION=mysql
DB_HOST={{ db_host }}
DB_PORT=3306
DB_DATABASE={{ mysql_database }}
DB_USERNAME={{ mysql_user }}
DB_PASSWORD={{ mysql_password }}

CACHE_DRIVER=file
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120

REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=smtp
MAIL_HOST=smtp.mailtrap.io
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS=null
MAIL_FROM_NAME="{{ app_name }}"



